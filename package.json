  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/bluebird": "^3.5.38",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16",
    "eslint": "^8.38.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
